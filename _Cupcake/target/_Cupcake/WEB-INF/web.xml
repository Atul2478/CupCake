<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>primeController</servlet-name>
        <servlet-class>Backend.PrimeController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>primeController</servlet-name>
        <url-pattern>/Backend.PrimeController</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
    <welcome-file>Backend.PrimeController</welcome-file>
  </welcome-file-list>
</web-app>
